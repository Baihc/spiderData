<script type="text/javascript">
    var s = 0;
    function gd() {
        if (document.documentElement && document.documentElement.scrollTop) {//如果定义文档类型
            a = document.documentElement.scrollTop;
        }
        else if (document.body) {//如果没有定义文档类型
            a = document.body.scrollTop;
        }
        p = 0.1 * (a - s);
        document.getElementById("kefu").style.top = s + "px";
        s = s + p;
    }
    setInterval("gd()", 1);
</script>
